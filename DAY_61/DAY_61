    .port_info 1 /INPUT 1 "rst";
    .port_info 2 /OUTPUT 1 "dout";
P_0x55c0e633d330 .param/l "period" 0 4 3, +C4<00000000000000000000000000000101>;
v0x55c0e6300bb0_0 .net "clk", 0 0, v0x55c0e6351140_0;  1 drivers
v0x55c0e6350c60_0 .var/i "count", 31 0;
v0x55c0e6350d40_0 .var "dout", 0 0;
v0x55c0e6350e10_0 .var "next", 0 0;
v0x55c0e6350ed0_0 .net "rst", 0 0, v0x55c0e63512b0_0;  1 drivers
v0x55c0e6350fe0_0 .var/i "ton", 31 0;
E_0x55c0e633d770 .event posedge, v0x55c0e6300bb0_0;
    .scope S_0x55c0e633d150;
T_0 ;
    %wait E_0x55c0e633d770;
    %load/vec4 v0x55c0e6350ed0_0;
    %cmpi/e 1, 0, 1;
    %jmp/0xz  T_0.0, 4;
    %pushi/vec4 0, 0, 32;
    %assign/vec4 v0x55c0e6350c60_0, 0;
    %pushi/vec4 0, 0, 32;
    %assign/vec4 v0x55c0e6350fe0_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x55c0e6350e10_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x55c0e6350c60_0;
    %load/vec4 v0x55c0e6350fe0_0;
    %cmp/s;
    %flag_or 5, 4;
    %jmp/0xz  T_0.2, 5;
    %load/vec4 v0x55c0e6350c60_0;
    %addi 1, 0, 32;
    %assign/vec4 v0x55c0e6350c60_0, 0;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x55c0e6350d40_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x55c0e6350e10_0, 0;
    %jmp T_0.3;
T_0.2 ;
    %load/vec4 v0x55c0e6350c60_0;
    %cmpi/s 5, 0, 32;
    %jmp/0xz  T_0.4, 5;
    %load/vec4 v0x55c0e6350c60_0;
    %addi 1, 0, 32;
    %assign/vec4 v0x55c0e6350c60_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x55c0e6350d40_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x55c0e6350e10_0, 0;
    %jmp T_0.5;
T_0.4 ;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x55c0e6350e10_0, 0;
    %pushi/vec4 0, 0, 32;
    %assign/vec4 v0x55c0e6350c60_0, 0;
T_0.5 ;
T_0.3 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x55c0e633d150;
T_1 ;
    %wait E_0x55c0e633d770;
    %load/vec4 v0x55c0e6350ed0_0;
    %cmpi/e 0, 0, 1;
    %jmp/0xz  T_1.0, 4;
    %load/vec4 v0x55c0e6350e10_0;
    %cmpi/e 1, 0, 1;
    %jmp/0xz  T_1.2, 4;
    %load/vec4 v0x55c0e6350fe0_0;
    %cmpi/s 5, 0, 32;
    %jmp/0xz  T_1.4, 5;
    %load/vec4 v0x55c0e6350fe0_0;
    %addi 1, 0, 32;
    %assign/vec4 v0x55c0e6350fe0_0, 0;
    %jmp T_1.5;
T_1.4 ;
    %pushi/vec4 0, 0, 32;
    %assign/vec4 v0x55c0e6350fe0_0, 0;
T_1.5 ;
T_1.2 ;
T_1.0 ;
    %jmp T_1;
    .thread T_1;
    .scope S_0x55c0e62ff350;
T_2 ;
    %vpi_call/w 3 15 "$dumpfile", "dump.vcd" {0 0 0};
    %vpi_call/w 3 16 "$dumpvars", 32'sb00000000000000000000000000000000 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55c0e6351140_0, 0, 1;
    %end;
    .thread T_2;
    .scope S_0x55c0e62ff350;
T_3 ;
T_3.0 ;
    %delay 5000, 0;
    %load/vec4 v0x55c0e6351140_0;
    %inv;
    %store/vec4 v0x55c0e6351140_0, 0, 1;
    %jmp T_3.0;
